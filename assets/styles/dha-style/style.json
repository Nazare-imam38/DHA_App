{
  "version": 8,
  "name": "DHA Base Map",
  "sources": {
    "dha-tiles": {
      "type": "vector",
      "url": "mbtiles://{dha-tiles}"
    }
  },
  "sprite": "",
  "glyphs": "fonts/{fontstack}/{range}.pbf",
  "layers": [
    {
      "id": "background",
      "type": "background",
      "paint": { "background-color": "#e6e6e6" }
    },
    {
      "id": "dha-fill",
      "type": "fill",
      "source": "dha-tiles",
      "source-layer": "layer0",
      "paint": {
        "fill-color": "#5c9bd3",
        "fill-opacity": 0.6
      }
    },
    {
      "id": "dha-outline",
      "type": "line",
      "source": "dha-tiles",
      "source-layer": "layer0",
      "paint": {
        "line-color": "#1f4e79",
        "line-width": 1.2
      }
    }
  ]
}
